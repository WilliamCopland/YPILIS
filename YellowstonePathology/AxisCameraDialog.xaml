<Window x:Class="YellowstonePathology.AxisCameraDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YellowstonePathology"
        mc:Ignorable="d"
        Title="Axis Camera Dialog" Height="300" Width="300">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Click="ButtonOpen_Click" Width="100" Height="30" Margin="2,2,2,2">Show</Button>
            <Button Click="ButtonClose_Click" Width="100" Height="30" Margin="2,2,2,2">Hide</Button>
        </StackPanel>
    </Grid>
</Window>
